CONFIG_TOYBOX=y
CONFIG_STATIC=y
CONFIG_ANDROID=y
CONFIG_TOYBOX_PREFIX="/system/bin"

# Basic commands
CONFIG_ECHO=y
CONFIG_LS=y
CONFIG_CAT=y
CONFIG_CP=y
CONFIG_MV=y
CONFIG_RM=y
CONFIG_MKDIR=y
CONFIG_RMDIR=y
CONFIG_CHMOD=y
CONFIG_CHOWN=y

# System
CONFIG_PS=y
CONFIG_KILL=y
CONFIG_DATE=y
CONFIG_TIME=y
CONFIG_UNAME=y

# Text
CONFIG_GREP=y
CONFIG_SED=y
CONFIG_AWK=y

# Network
CONFIG_PING=y
CONFIG_IFCONFIG=y

# DISABLED
# CONFIG_SU is not set
# CONFIG_LOGIN is not set
# CONFIG_PASSWD is not set
